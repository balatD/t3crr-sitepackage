page = PAGE
page {
  typeNum = 0

  shortcutIcon = {$resourcePublic}/Icons/favicon.ico

  bodyTag >
  bodyTagCObject = TEXT
  bodyTagCObject {
    field = uid
    wrap = <body class="hyphenate cpi uid-|">
  }
}

page.10 = FLUIDTEMPLATE
page.10 {
  templateRootPaths.0 = EXT:t3crr_sitepackage/Resources/Private/Templates/Page/
  layoutRootPaths.0 = EXT:t3crr_sitepackage/Resources/Private/Layouts/Page/
  partialRootPaths.0 = EXT:t3crr_sitepackage/Resources/Private/Partials/Page/

  extbase.controllerExtensionName = t3crr_sitepackage

  templateName = TEXT
  templateName {
    cObject = TEXT
    cObject {
      data = pagelayout
      required = 1
      case = uppercamelcase
      split {
        token = pagets__t3crr_sitepackage_
        cObjNum = 1
        1.current = 1
      }
    }

    ifEmpty = Default
  }

  settings {
  }

  variables {
  }

  dataProcessing {
    10 = TYPO3\CMS\Frontend\DataProcessing\MenuProcessor
    10 {
      levels = 2
      as = mainNavigation
    }

    20 = TYPO3\CMS\Frontend\DataProcessing\MenuProcessor
    20 {
      levels = 1
      special = directory
      special.value = 42
      as = metaNavigation
    }

    30 = TYPO3\CMS\Frontend\DataProcessing\MenuProcessor
    30 {
      levels = 2
      special = directory
      special.value = 5
      as = footerNavigation
    }
  }
}
